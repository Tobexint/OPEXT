{
  "manifest_version": 3,
  "name": "Tab Manager",
  "version": "1.0",
  "description": "Manage and organize your tabs effectively.",
  "permissions": [
    "tabs",
    "activeTab",
    "storage"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "host_permissions": [
    "http://localhost:5000/process-tabs",
    "http://localhost:5000"
  ]
}

